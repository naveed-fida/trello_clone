@import 'https://fonts.googleapis.com/css?family=Roboto'
@import "whitespace-reset.css"
body
  background: #d29034
  font: normal normal 14px "Roboto", Arial, Helvetica, sans-serif
  color: #555;
  #main
    padding: 15px
    overflow: hidden;
  a
    text-decoration: none
    color: #000;
.list
  background: #E2E4E6
  width: 300px
  // padding: 10px 10px 0 10px
  border-radius: 4px
  overflow: auto
  float: left
  margin: 0 15px 0 0
  ul
    padding: 0 10px 0 10px
  h2
    font-size: 16px
    padding: 10px
  li
    padding: 10px
    background: #fff
    box-sizing: border-box
    margin-bottom: 8px
    border-radius: 4px
    box-shadow: 0px 1px 1px #ccc;
  a
    display: block
    color: #888
    padding: 10px
    border-radius: inherit
    &:hover
      background: #CDD2D4;